set xrange[-2:8]
set yrange[-2:5]

# lagrange
f(x) = ((x**3-13*x**2+50*x-56)/-56) + 3*((x**3 - 11*x**2 + 28*x)/20) + (-1)*((x**3-9*x**2+14*x)/-24) + 4*((x**3-6*x**2+8*x)/105)
# newton
g(x) = 1 + x - 0.75*(x-2)*x + (x-4)*(x-2)*x*0.21190476
# linear
h(x) = 1 + 2.5*x - 1.3554421769*x**2 + 0.1513605442*x**3

plot f(x), g(x), h(x), 'points.txt' w p ls 1